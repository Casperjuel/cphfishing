{
  "name": "style_taskrunner",
  "version": "1.0.1",
  "description": "Base style auto compiler",
  "dependencies": {
    "node-bourbon": "*",
    "node-neat": "*",
    "node-sass": "*",
    "wp-timber-cli": "^0.4.1"
  },
  "devDependencies": {},
  "scripts": {
    "watch": "node-sass --include-path node_modules/bourbon/app/assets/stylesheets --include-path node_modules/bourbon-neat/app/assets/stylesheets -w --source-map true --output-style compressed sass -o css"
  },
  "author": "Casper Juel @ info@casperjuel.dk"
}
